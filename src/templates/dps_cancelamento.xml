<?xml version="1.0" encoding="UTF-8"?>
<pedRegEvento xmlns="http://www.sped.fazenda.gov.br/nfse" versao="1.00">
  <infPedReg Id="{{id}}">
    <tpAmb>{{ambiente}}</tpAmb>
    <verAplic>{{versaoAplicacao}}</verAplic>
    <dhEvento>{{dataHoraEvento}}</dhEvento>
    {{#if cnpjAutor}}<CNPJAutor>{{cnpjAutor}}</CNPJAutor>{{/if}}
    {{#if cpfAutor}}<CPFAutor>{{cpfAutor}}</CPFAutor>{{/if}}
    <chNFSe>{{chaveAcesso}}</chNFSe>
    <e101101>
      <xDesc>Cancelamento de NFS-e</xDesc>
      <cMotivo>{{codigoMotivo}}</cMotivo>
      <xMotivo>{{descricaoMotivo}}</xMotivo>
    </e101101>
  </infPedReg>
</pedRegEvento>